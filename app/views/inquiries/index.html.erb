<p id="notice"><%= notice %></p>

<h1>Inquiries</h1>

<table>
  <thead>
    <tr>
      <th>Full name</th>
      <th>Phone number</th>
      <th>Email</th>
      <th>Store name</th>
      <th>Domain name</th>
      <th>Preffered name</th>
      <th>Plan</th>
      <th>Billing type</th>
      <th>Web administration</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inquiries.each do |inquiry| %>
      <tr>
        <td><%= inquiry.full_name %></td>
        <td><%= inquiry.phone_number %></td>
        <td><%= inquiry.email %></td>
        <td><%= inquiry.store_name %></td>
        <td><%= inquiry.domain_name %></td>
        <td><%= inquiry.preffered_name %></td>
        <td><%= inquiry.plan %></td>
        <td><%= inquiry.billing_type %></td>
        <td><%= inquiry.web_administration %></td>
        <td><%= link_to 'Show', inquiry %></td>
        <td><%= link_to 'Edit', edit_inquiry_path(inquiry) %></td>
        <td><%= link_to 'Destroy', inquiry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inquiry', new_inquiry_path %>
