<!DOCTYPE html>
<html>
  <head>
    <title>Namarunu - Build your online store. Fast</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="Namarunu">
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-173158853-3"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    gtag('js', new Date());

    gtag('config', 'UA-173158853-3');
    function gtag(){dataLayer.push(arguments);}
  </script>
</head>


  <body>
      <%= render 'shared/navbar' %>
      <div class="body">
      <%= yield %>

      <%= render 'shared/footer' %>
    </div>
    <!-- Start of HubSpot Embed Code -->
  <script type="text/javascript" id="hs-script-loader" async defer src="//js.hs-scripts.com/8749832.js"></script>
<!-- End of HubSpot Embed Code -->
  </body>
</html>
